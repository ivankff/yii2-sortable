{
    "name": "ivankff/yii2-sortable",
    "description": "yii2 tools for position column",
    "type": "yii2-extension",
    "keywords": ["yii2", "yii 2", "behaviors", "sortable", "position"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "ivankff",
            "email": "ivankff@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/ivankff/yii2-sortable/issues?state=open",
        "source": "https://github.com/ivankff/yii2-sortable"
    },
    "require": {
        "yiisoft/yii2": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "autoload": {
        "psr-4": {
            "ivankff\\yii2Sortable\\": "src/"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}